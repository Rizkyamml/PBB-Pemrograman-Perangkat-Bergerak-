<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rizkyamalia.hmjpolines">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="My Favorite Film"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.rizkyamalia.hmjpolines.aboutme"
            android:label="About Me"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name="com.rizkyamalia.hmjpolines.DetailListHmj"
            android:parentActivityName="com.rizkyamalia.hmjpolines.MainActivity"/>
        <activity android:name="com.rizkyamalia.hmjpolines.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>